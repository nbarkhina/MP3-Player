    "version": "0.2.0",
    "configurations": [


        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to Chrome",
            "port": 9222,
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/scannercore/bin/Debug/net5.0/scannercore.dll",
            "args": [],
            "cwd": "${workspaceFolder}/scannercore",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
    ]
}